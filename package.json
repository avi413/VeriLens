{
  "name": "verilens-mvp",
  "version": "0.1.0",
  "private": true,
  "exports": {
    "./sdk": {
      "types": "./dist/sdk/index.d.ts",
      "default": "./dist/sdk/index.js"
    },
    "./sdk/*": {
      "types": "./dist/sdk/*",
      "default": "./dist/sdk/*"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node --project tsconfig.json app/api/httpClient.ts",
    "test": "jest --config jest.config.ts --runInBand",
    "mobile-camera": "node app/mobile-camera/server.js",
    "camera": "npm run mobile-camera",
    "web": "node app/web/web-server.js",
    "start:web": "npm run build && npm run web"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "exif-parser": "^0.1.12",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
